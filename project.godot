config_version=5

[application]

config/name="Clawping"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Clawping="*res://src/Clawping.gd"
Pooper="*res://3DPoop/pooper.tscn"

[editor_plugins]

enabled=PackedStringArray()

[filesystem]

